@model AddLayoutViewModel
@{
    ViewData["Title"] = "AddLayout";
    Layout = "~/Views/AdminLayout/_Adminlayout.cshtml";
}

<form method="post">


    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <label>Footer Partial Code</label>
    <input type="text" asp-for="FooterPartial" class="form-control" />
    <label>Header Partial Code</label>
    <input type="text" asp-for="HeaderPartial" class="form-control" /> 
    <label>Head Partial Code</label>
    <input type="text" asp-for="HeadPartial" class="form-control" /> 
    <label>Script Partial Code</label>
    <input type="text" asp-for="ScriptPartial" class="form-control" /> 
   

    <button class="btn btn-primary">Add Layout</button>


</form>

